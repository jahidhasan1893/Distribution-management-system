        /* Style the navigation bar links */
        .navbar1 a {
            float: left;
            display: block;
            color: white;
            text-align: center;
            padding: 14px 16px;
            text-decoration: none;
            font-size: 14px;
            height: 70px;
        }

        /* Change the color of the links when you hover over them */
        .navbar1 a:hover {
            background-color: #ddd;
            color: #333;
        }

        /* Add a logo to the left side of the navbar */
        .navbar1 .logo {
            float: left;

        }

        /* Style the logo image */
        .navbar1 .logo img {


            max-width: 60px;
            max-height: 60px;
        }

        /* Style a button on the right side of the navbar */
        .navbar1 .btn {
            float: right;
            background-color: #4CAF50;
            color: white;
            border: none;
            padding: 10px 20px;
            margin: 10px 10px;
            border-radius: 5px;
            cursor: pointer;
        }

        /* Add a search icon on the right side of the navbar */
        .navbar1 .search-icon {
            float: right;
            font-size: 20px;
            margin: 10px 10px;
            color: white;
            cursor: pointer;
        }

        /* Add responsiveness to the navigation bar */
        @media screen and (max-width: 1200px) {

            .navbar1 a,
            .navbar1 .btn,
            .navbar1 .search-icon {
                float: none;
                display: block;
                text-align: left;
                width: 100%;
                padding: 10px;
            }
        }

        /* Style the search input field */
        .navbar1 #search-field {
            float: left;
            width: 560px;
            margin: 10px 10px;
            padding: 10px;
            border-radius: 5px;
        }

        /* Style the search icon */
        .navbar1 .search-icon {
            float: right;
            font-size: 20px;
            margin: 10px 10px;
            color: white;
            cursor: pointer;
            margin-top: 17px;
        }

        /* Add responsiveness to the navigation bar */
        @media screen and (max-width: 600px) {

            .navbar1 #search-field,
            .navbar1 .search-icon {
                display: block;
            }
        }

        /* Add a button to reject the search bar */
        .navbar1 #reject-search {
            float: left;
            margin: 10px 10px;
            background-color: #ff4f4f;
            color: white;
            border: none;
            padding: 10px;
            border-radius: 5px;
            cursor: pointer;
        }

        /* Add responsiveness to the navigation bar */
        @media screen and (max-width: 600px) {

            .navbar1 #search-field,
            .navbar1 .search-icon,
            .navbar1 #reject-search {
                display: block;
            }
        }

        footer {
            color: #fff;
            padding: 30px 0;
            text-align: center;
            width: 100%;
        }

        .social-icons {
            list-style: none;
            padding: 0;
            margin-right: 140px;
        }

        .social-icons li {
            display: inline-block;
            margin: 0 15px;
        }

        .social-icons a {
            color: #fff;
            text-decoration: none;
            font-size: 24px;
            transition: color 0.3s;
        }

        .social-icons a:hover {
            color: #007bff;
            /* Change to your preferred hover color */
        }

        footer a {
            margin-top: 20px;
            font-size: 14px;
        }

        .social-icons a {
            padding: 8px;
            color: #707070;
            line-height: 22px;
            letter-spacing: 0.01em;
        }

        .feature-box .feature-header h3 {
            margin-bottom: 13%;
            color: #282828;
            font-weight: 600;
            line-height: 24px;
            font-family: 'Montserrat', sans-serif;
            font-size: 14px;
            margin-left: 6px;
            text-align: center;
        }

        .feature-box .feature-text a {
            text-decoration: none;
            color: #707070;
            font-size: 14px;
            letter-spacing: 0.01em;
            text-align: center;
        }

        .feature-box .feature-text {
            text-decoration: none;
            color: #707070;
            font-size: 14px;
            letter-spacing: 0.01em;
            text-align: center;
        }

        .feature-box .feature-header {
            display: inline-block;
            vertical-align: middle;
        }

        .feature-box .feature-header i {
            color: #707070;
        }

        .container2 {
            display: flex;
            justify-content: space-around;
            align-items: center;
            flex-wrap: wrap;
        }

        .card2 {
            margin: 10px;
            background-color: #fff;
            border-radius: 10px;
            box-shadow: 0 2px 20px rgba(0, 0, 0, 0.2);
            overflow: hidden;
            width: 300px;
        }

        .card-header img {
            width: 100%;
            height: 200px;
            object-fit: cover;
        }

        .card-body {
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: flex-start;
            padding: 20px;
            min-height: 250px;
            text-align: left;
        }

        .tag {
            background: #cccccc;
            border-radius: 50px;
            font-size: 12px;
            margin: 0;
            color: #fff;
            padding: 2px 10px;
            text-transform: uppercase;
            cursor: pointer;
        }

        .tag-teal {
            background-color: #47bcd4;
        }

        .tag-purple {
            background-color: #5e76bf;
        }

        .tag-pink {
            background-color: #cd5b9f;
        }

        .card-body p {
            font-size: 13px;
            margin: 0 0 40px;
        }

        .user {
            display: flex;
            margin-top: auto;
        }

        .user img {
            border-radius: 50%;
            width: 40px;
            height: 40px;
            margin-right: 10px;
        }

        .user-info h5 {
            margin: 0;
        }

        .user-info small {
            color: #545d7a;
        }

        .btn:hover {
            color: rgb(182, 57, 7);
        }

        .gallery {
            background-color: #FFFFFF;
            width: 100%;
            padding-bottom: 100px;
            padding-top: 80px;
            display: flex;
            flex-wrap: wrap;
            justify-content: space-between;

            padding-left: 250px;
            padding-right: 250px;

            margin: 0 auto;

        }

        .gallery-item {
            flex-basis: calc(33.33% - 20px);
            /* Adjust the width as needed */
            margin-bottom: 20px;
            /* Adjust the margin to control the gap between items */
            overflow: hidden;
        }

        .gallery-item img {
            width: 100%;
            height: auto;
            transition: transform 0.3s;
            flex-grow: 1;
            /* Allow the image to grow within the container */
            object-fit: cover;
            /* Maintain aspect ratio and fill the container */
        }

        .gallery-item:hover img {
            transform: scale(1.1);
        }

        @import url(https://fonts.googleapis.com/css?family=Open+Sans:400,700);





        .navbar1 {
            z-index: 1;
            display: flex;
            justify-content: space-around;
            background-color: #333;
            /* overflow: hidden; */
            position: fixed;
            top: 0;
            width: 100%;
            font-family: Arial, sans-serif;
            height: 70px;
        }



        .navbar1 .right .dropdown {
            margin-left: 20px;
            width: 130px;
            display: inline-block;
            margin-right: 10px;
            position: relative;

        }

        .navbar1 .right .dropdown>a {
            margin-top: 7px;
            border-radius: 50%;
            /* Make the image circular */
            overflow: hidden;
            /* Hide overflow for circle */
            box-shadow: 0 6px 5px -5px rgba(0, 0, 0, 0.3);
            display: flex;
            /* Use flexbox for centering */
            justify-content: center;
            /* Horizontally center the content */
            align-items: center;
            /* Vertically center the content */
            width: 40px;
            /* Adjust the width to your liking */
            height: 40px;
            /* Adjust the height to your liking */
        }

        .navbar1 .right .dropdown>a img {
            width: 80px;
            /* Ensure the image takes the full space inside the circle */
            height: 80px;
            /* object-fit: cover; */
            /* Keep the image's original size within the circular container */
        }

        .navbar1 .right .dropdown ul {

            list-style-type: none;
            display: block;
            margin: 0;
            padding: 0;
            position: absolute;
            width: 100%;
            box-shadow: 0 6px 5px -5px rgba(0, 0, 0, 0.3);
            overflow: hidden;
        }

        .navbar1 .right .dropdown ul a {
            width: 100%;
            text-align: left;
            display: block;
            padding: 0 0 0 10px;
            text-decoration: none;
            line-height: 40px;
            font-size: 13px;
            text-transform: uppercase;
            font-weight: bold;
            color: #999;
            background-color: #FFF;
        }

        .navbar1 .right .dropdown li {

            height: 0;
            overflow: hidden;
            transition: all 500ms;
        }

        .navbar1 .right .dropdown.hover li {
            transition-delay: 300ms;
        }

        .navbar1 .right .dropdown li:first-child a {
            border-radius: 2px 2px 0 0;
        }

        .navbar1 .right .dropdown li:last-child a {
            border-radius: 0 0 2px 2px;
        }

        .navbar1 .right .dropdown li:first-child a::before {
            content: "";
            display: block;
            position: absolute;
            width: 0;
            height: 0;
            border-left: 10px solid transparent;
            border-right: 10px solid transparent;
            border-bottom: 10px solid #FFF;
            margin: -10px 0 0 30px;
        }

        .navbar1 .right .dropdown a:hover {
            background-color: #EEE;
            color: #666;
        }

        .navbar1 .right .dropdown.hover:hover li {
            height: 40px;
        }

        .navbar1 .right .dropdown.hover:hover li:first-child {
            padding-top: 65px;
        }


        .right5 {
            width: 600px;
            background-color: rgba(255, 255, 255, 0.9);
            padding: 40px;
            border-radius: 5px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
            text-align: center;

        }
        .left5 {
            width: 350px;
            background-color: rgba(255, 255, 255, 0.9);
            padding: 40px;
            border-radius: 5px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
            text-align: center;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
        }

        .right5 h1 {
            margin-top: 0;
            color: #333;
        }
        .left5 h3 {
            margin-top: 0;
            color: #333;
        }

        .input-container5 {
            margin: 10px 0;
            display: flex;
            justify-content: center;
            align-items: center;
            column-gap: 10px;
            font-size: small;
            width: 480px;
        }

        

        .right5 input {
            width: 478px;
            padding: 10px;
            border: 1px solid #ccc;
            border-radius: 5px;
        }

        .right5 select {
            width: 152px;
            padding: 10px;
            border: 1px solid #ccc;
            border-radius: 5px;
        }

        .right5 button,.left5 button {
            background: #00a859;
            color: #fff;
            border: none;
            border-radius: 5px;
            padding: 10px 40px;
            cursor: pointer;
            transition: background-color 0.3s ease;
        }

        .right5  button:hover,.left5 button:hover {
            background-color: #00894f;
        }

    </style>

<body>
    <div class="navbar1" style="font-family: 'Poppins', sans-serif;">
        <div class="left">
            <div class="logo">
                <img src="images/Final-Pusti-Bangla-logo-trans-web-sm.png" alt="Logo">
            </div>
        </div>

        <div class="right">
            <a href="#" style="margin-top: 10px;height: 50px;" id="dashboard">Dashboard</a>

            <div class="dropdown hover">
                <a href="#">
                    <img src="<%= dp %>" alt="" style="object-fit: fill;">
                </a>
                <ul>
                    <li><a href="#">Item</a></li>
                    <li><a href="#" id="profile">Profile</a></li>
                    <li><a href="#" id="cart">Your Cart</a></li>
                    <li><a href="#" id="orders">Orders</a></li>
                    <li><a href="#" id="requests">Requests</a></li>
                    <li><a href="#" id="log-out">Log Out</a></li>

                </ul>
            </div>
            <!-- <span class="search-icon" onclick="toggleSearch()"><i class='bx bx-search-alt-2'></i></span>
            <input type="text" id="search-field" style="display: none;" placeholder="Search...">
            <button id="reject-search" onclick="rejectSearch()"><i class='bx bx-x ' ></i></button> -->
        </div>

    </div>


    <div class="bg" data-img-size="(W)1920px X (H)800px"
    style=" background: linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0)) repeat scroll 0% 0%, transparent url('images/bg1.png') repeat scroll 0% 0%;background-attachment: fixed;background-size: cover;width: 100%;display: flex;justify-content: center;align-items: center;">
        <div style="display:flex;justify-content: space-between;width: 950px;height: 80vh;font-family: 'Arial', sans-serif;margin-top: 160px;margin-bottom: 160px;">
            <div class="left5" >
                
                <img src="<%= dp %>" alt="" style="max-width: 90px;max-height: 90px;border-radius: 50%;background-image: var(--img-url), linear-gradient(to bottom right, #ff3cac, #562b7c, #2b86c5);flex-shrink: 0;
                margin: 20px;
                height: calc(150px + 6vw);
                width: calc(150px + 6vw);
                border: calc(8px + 0.2vw) solid transparent;
                background-origin: border-box;
                background-clip: content-box, border-box;
                background-size: cover;
                box-sizing: border-box;
                box-shadow: 0 0 5px 5px rgba(0, 0, 0, 0.5);">
                <h3 style="padding-top: 0px;"><%= firstname %> <%= lastname %></h3>
                <button style="margin-top: 10px;">Update Photo</button>
            </div>
            <div class="right5">
                <form id="login-form">
                    <h1>Update Profile</h1>
                    <div class="input-container5">
                        <input type="text" id="shopname" placeholder="shop name" value="<%= firstname %>">
                        <input type="text" id="shopname" placeholder="shop name" value=" <%= lastname %>">
                        <!-- <i class='bx bx-user'></i> -->
                    </div>
                  
                    <div class="input-container5">
                        <input type="text" id="nid" placeholder="NID number" value="<%= nid %>">
                        <!-- <i class='bx bxs-envelope'></i> -->
                    </div>
                    <div class="input-container5">
                        <input type="text" id="licence" placeholder="Distributor licence number" value="<%= licence %>">
                        <!-- <i class='bx bxs-envelope'></i> -->
                    </div>
    
                    <div class="input-container5">
                        <input type="text" id="phonenumber" placeholder="phone number" value="<%= phonenumber %>">
                        <!-- <i class='bx bxs-envelope'></i> -->
                    </div>
                    <div class="input-container5">
                        <select name="divisions" id="divisions" onchange="divisionsList();" >
                            <option disabled selected>Select Division</option>
                            <option value="Barishal">Barishal</option>
                            <option value="Chattogram">Chattogram</option>
                            <option value="Dhaka">Dhaka</option>
                            <option value="Khulna">Khulna</option>
                            <option value="Mymensingh">Mymensingh</option>
                            <option value="Rajshahi">Rajshahi</option>
                            <option value="Rangpur">Rangpur</option>
                            <option value="Sylhet">Sylhet</option>
                        </select>
                        <select name="" id="distr" onchange="thanaList();"></select>
                        <select name="" id="polic_sta"></select>
                        <!-- <i class='bx bxs-envelope'></i> -->
                    </div>
                    <div class="input-container5">
                        <input type="text" id="address" placeholder="shop address" value="<%= address %>">
                        <!-- <i class='bx bxs-envelope'></i> -->
                    </div>
                    <div class="input-container5">
                        <input type="password" id="password" placeholder="update password" value="<%= password %>">
                        <!-- <i class='bx bxs-lock-alt' ></i> -->
                    </div>
                    <div class="input-container5">
                        <input type="password" id="password" placeholder="confirm password" value="<%= password %>">
                        <!-- <i class='bx bxs-lock-alt' ></i> -->
                    </div>
                    <button type="submit">Update</button>
                </form>
            </div>
        </div>
    </div>

    

    




    <footer style="width: 100%;background-color: #ffffff;padding: 60px;box-sizing: border-box;">
        <!-- social elements -->
        <div class="social-icons" style="text-align: right;">
            <a href="#" class="margin-eight-right">
                <i class='bx bxl-facebook bx-sm'></i>
            </a>
            <a href="#" class="margin-eight-right">
                <i class='bx bxl-twitter bx-sm'></i>
            </a>
            <a href="#" class="margin-eight-right">
                <i class='bx bxl-google-plus bx-sm'></i>
            </a>
            <a href="#" class="margin-eight-right">
                <i class='bx bxl-pinterest bx-sm'></i>
            </a>
            <a href="#" class="margin-eight-right">
                <i class='bx bxl-linkedin bx-sm'></i>
            </a>
            <a href="#">
                <i class='bx bxl-youtube bx-sm'></i>
            </a>
        </div>
        <!-- end social elements -->
    </footer>
    <script src="js/change.js"></script>
    <script>
        
        document.getElementById('divisions').value = '<%= division %>';
        divisionsList()
        document.getElementById('distr').value = '<%= district %>';
        thanaList()
        document.getElementById('polic_sta').value = '<%= policestation %>';

        document.getElementById('dashboard').addEventListener('click', function () {
            window.location.href = '/distributor_dashboard?id=<%= user?.id%>';
        });
        

        document.getElementById('cart').addEventListener('click', function () {
            window.location.href = '/distributors/cart?id=<%= user?.id%>';
        });

        document.getElementById('requests').addEventListener('click', function () {
            window.location.href = '/distributors/requests?id=<%= user?.id%>';
        });

        document.getElementById('orders').addEventListener('click', function () {
            window.location.href = '/distributors/orders?id=<%= user?.id%>';
        });

        document.getElementById('log-out').addEventListener('click', function () {
            window.location.href = '/customer_logout';
        });


        

       
    </script>
